class Car {
    String brand;
    int year;
    Car(String b, int y) {
        brand = b;
        year = y;
    }
    void displayInfo() {
        System.out.println("Brand: " + brand + ", Year: " + year);
    }
}

public class Main {
    public static void main(String[] args) {
        Car c1 = new Car("Toyota", 2020);
        c1.displayInfo();
    }
}
